{
  "cli": {
    "version": ">= 12.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium"
      }
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium",
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "production": {
      "channel": "production",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium",
        "buildType": "app-bundle"
      },
      "env": {
        "NODE_ENV": "production",
        "EXPO_USE_HERMES": "true"
      }
    },
    "testflight": {
      "extends": "production",
      "channel": "production",
      "distribution": "store",
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Release"
      },
      "env": {
        "NODE_ENV": "production",
        "EXPO_USE_HERMES": "true"
      }
    },
    "development-dev": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium"
      },
      "env": {
        "APP_CONFIG": "development"
      }
    },
    "preview-dev": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium"
      },
      "env": {
        "APP_CONFIG": "development"
      }
    },
    "production-dev": {
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium"
      },
      "env": {
        "APP_CONFIG": "development"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "junseopsan@gmail.com",
        "ascAppId": "6746711838",
        "appleTeamId": "JG26695UN3"
      }
    },
    "production-dev": {
      "ios": {
        "appleId": "junseopsan@gmail.com",
        "ascAppId": "6749815418",
        "appleTeamId": "JG26695UN3"
      }
    }
  }
}
